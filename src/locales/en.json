    "user": {
      "nav": {
        "profile": "Profile",
        "account": "Account",
        "preferences": "Preferences"
      },
      "preferences": {
        "title": "My Preferences",
        "language": {
          "subtitle": "Language"
        },
        "theme": {
          "subtitle": "Theme"
        }
      }
    },
    "groupsList": {
      "title": "My Groups",
      "noResults": {
        "label": "You don't have any groups yet!",
        "message": "Create one now to invite your friends",
        "action": "Create My First Group"
      },
      "creation": {
        "card": {
          "title": "New group",
          "actions": {
            "submit": "Create"
          }
        },
        "form": {
          "name": {
            "field": "Group name",
            "rules": {
              "required": "A name is required"
            }
          },
          "email": {
            "field": "Guest email",
            "rules": {
              "required": "An email is required",
              "format": "Guest email must be valid"
            }
          }
        }
      }
    },
    "groupDetail": {
      "noResults": {
        "label": "Group not found",
        "message": "Don't stay lost. You should explore your own groups",
        "action": "See my groups"
      },
      "nav": {
        "overview": "Overview",
        "members": "Members",
        "plugins": "Plugins",
        "settings": "Settings"
      }
    },
    "groupMembersList": {
      "headers": {
        "guests": "Guests ({counter})",
        "members": "Members ({counter})"
      }
    }
  },
  "dialog": {
    "myInvitations": {
      "card": {
        "title": "My Invitations",
        "noResults": {
          "label": "No invitation received",
          "message": "Come back later",
          "action": "Close"
        },
        "listItem": {
          "subtitle": "Invited by {owner}",
          "actions": {
            "label": "Accept ?",
            "reject": {
              "title": "Reject invitation ?",
              "text": "Are you sure to reject this invitation to the group \"{groupName}\" from {owner} ?",
              "confirm": "Confirm",
              "cancel": "Cancel"
            }
          }
        }
      }
    }
  },
  "form": {
    "userProfile": {
      "name": {
        "label": "Name",
        "rules": {
          "required": "A name is required"
        }
      },
      "bio": {
        "label": "Bio",
        "rules": {
          "format": "The bio must be less than {nbChars} characters"
        }
      }
    }
  },
  "listItem": {
    "guest": {
      "invitedBy": "Invited by {member}"
    }
  }
